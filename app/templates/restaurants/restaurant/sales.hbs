{{#nf-graph width=500 height=300 paddingRight=10}}
  {{#nf-graph-content}}
   
    {{nf-line data=model.salesData}}
  {{/nf-graph-content}}

    
  {{#nf-y-axis as |tick|}}
    <text>{{tick.value}}</text>
  {{/nf-y-axis}}


  {{#nf-x-axis as |tick|}}
    <text>{{tick.value}}</text>
  {{/nf-x-axis}}
{{/nf-graph}}
