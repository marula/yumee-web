{{#nf-graph width=500 height=300 paddingRight=10 paddingTop=10 xScaleType="ordinal" yMinMode="fixed"
    yMin=0}}
  {{#nf-graph-content}}
    
    {{nf-bars data=model.menuItemStockCount }}    
    
  {{/nf-graph-content}}

  {{#nf-y-axis as |tick|}}
    <text>{{tick.value}}</text>
  {{/nf-y-axis}}

  {{#nf-x-axis as |tick|}}
    <text>{{tick.value}}</text>
  {{/nf-x-axis}}

 {{/nf-graph}}